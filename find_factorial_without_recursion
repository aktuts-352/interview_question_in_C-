// Program to find the factorial of the two given number without using recursion;

#include<bits/stdc++.h>
using namespace std;

int findFactorial(int num1){
    int fact = 1;
    for(int i = 1; i <= num1; i++){
        fact = fact * i;
    }
    return fact;
}

int main(){
    int n1, n2;
    cin>>n1>>n2;
    int fact1 = findFactorial(n1);
    int fact2 = findFactorial(n2);
    cout<<"Factorial of "<<n1<<" is "<<fact1<<endl;
    cout<<"Factorial of "<<n2<<" is "<<fact2;
    return 0;
}
