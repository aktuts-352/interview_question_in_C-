// Program to take input of 2-D Array and print it in a matrix form

#include<bits/stdc++.h>
using namespace std;

int
main ()
{

  int n, m;
  cin >> n >> m;
  int a[n][m];
  for (int i = 0; i < n; i++)
    {
      for (int j = 0; j < m; j++)
	{
	  cin >> a[i][j];
	}
    }

  cout << "Output of the 2-D array in the form of matrix is " << endl;

  for (int i = 0; i < n; i++)
    {
      for (int j = 0; j < m; j++)
	{
	  cout << a[i][j] << " ";
	}
      cout << endl;
    }
  return 0;
}
